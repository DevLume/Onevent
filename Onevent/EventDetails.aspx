                <h1><%#:Item.EventName %></h1>
            </div>
            <div class="card-body">
            
            <br />
            <table>
                <tr>
                    <td>
                        <img src="/Catalog/Images/<%#:Item.ImagePath %>" style="border:solid; height:300px" alt="<%#:Item.EventName %>"/>
                    </td>
                    <td>&nbsp;</td>  
                    <td style="vertical-align: top; text-align:left;">
                        <b>Aprašas:</b><br /><%#:Item.Description %>
                        <br />
                        <hr>
                        <span><b>Data:</b> 2018-10-18</span>
                        <br />
                        <hr>
                        <span><b>Kaina:</b>&nbsp;<%#: String.Format("{0:c}", Item.UnitPrice) %></span>
                        <br />
                        <hr>
                        <span><b>Renginio kodas:</b>&nbsp;<%#:Item.EventID %></span>
                        <br />
                        <hr>
                        <div class="form-inline">
                             <a href="#" class="button">Rezervuoti</a>
                             <a href="/AddToCart.aspx?eventID=<%#:Item.EventID %>" class="button-buy">Pirkti</a>
                        </div>

                        <td>
                        <img src="data:image/jpeg;base64,"
                            style="border:solid; height:300px" alt="<%#:Item.EventName %>"/>
                    </td>
                    </td>
                </tr>
            </table>
                </div>
                </div>
                </div>
        </ItemTemplate>
